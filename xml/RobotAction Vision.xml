<RobotAction>
    <!-- For Team Prop recognition include only the FIND_TEAM_PROP 
         element for each competition starting position. -->
    <!-- All spike window values under <team_prop_recognition> are relative to the image ROI.
         The left_window may enclose the LEFT_SPIKE or the CENTER_SPIKE, depending on the
         robot's starting position. For example, for BLUE_A2 the left_window encloses the
         CENTER_SPIKE. -->
    <OpMode id="BLUE_A2">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>front_webcam</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>118</x>
                        <y>270</y>
                        <width>515</width>
                        <height>159</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_pixel_count</recognition_path>
                    <left_window>
                        <width>335</width>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <OpMode id="BLUE_A4">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>front_webcam</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>32</x>
                        <y>282</y>
                        <width>520</width>
                        <height>142</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_bright_spot</recognition_path>
                    <left_window>
                        <width>180</width>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <OpMode id="RED_F2">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>front_webcam</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>16</x>
                        <y>288</y>
                        <width>532</width>
                        <height>142</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_pixel_count</recognition_path>
                    <left_window>
                        <width>200</width>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <OpMode id="RED_F4">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>front_webcam</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>101</x>
                        <y>275</y>
                        <width>519</width>
                        <height>162</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_bright_spot</recognition_path>
                    <left_window>
                        <width>337</width>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

</RobotAction>