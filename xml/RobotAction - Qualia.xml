<RobotAction>
    <!-- For Team Prop recognition include only the FIND_TEAM_PROP 
         element for each competition starting position. -->
    <!-- All spike window values under <team_prop_recognition> are relative to the image ROI.
         The left_window may enclose the LEFT_SPIKE or the CENTER_SPIKE, depending on the
         robot's starting position. For example, for BLUE_A2 the left_window encloses the
         CENTER_SPIKE. -->

    <!-- Qualia BLUE_A2 -->
    <OpMode id="BLUE_A2">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>WIN_20240113_16_00_38_Pro.jpg</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>123</x>
                        <y>290</y>
                        <width>512</width>
                        <height>76</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_pixel_count</recognition_path>
                    <left_window>
                        <width>382</width>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <!-- Qualia BLUE_A4 -->
    <OpMode id="BLUE_A4">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>WIN_20240113_15_58_18_Pro.jpg</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>0</x>
                        <y>300</y>
                        <width>583</width>
                        <height>82</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_pixel_count</recognition_path>
                    <left_window>
                        <width>150</width>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <!-- Qualia RED_F2 -->
    <OpMode id="RED_F2">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>WIN_20240113_11_36_18_Pro.jpg</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>0</x>
                        <y>283</y>
                        <width>456</width>
                        <height>81</height>
                    </image_roi>
                </image_parameters>
                <team_prop_recognition>
                    <recognition_path>color_channel_pixel_count</recognition_path>
                    <left_window>
                        <width>162</width>
                        <prop_location>LEFT_SPIKE</prop_location>
                    </left_window>
                    <right_window>
                        <prop_location>CENTER_SPIKE</prop_location>
                    </right_window>
                    <team_prop_npos>
                        <prop_location>RIGHT_SPIKE</prop_location>
                    </team_prop_npos>
                </team_prop_recognition>
            </FIND_TEAM_PROP>
        </actions>
    </OpMode>

    <!-- Qualia RED_F4 -->
    <OpMode id="RED_F4">
        <actions>
            <FIND_TEAM_PROP>
                <image_parameters>
                    <image_source>WIN_20240113_10_57_00_Pro.jpg</image_source>
                    <resolution>
                        <width>640</width>
                        <height>480</height>
                    </resolution>
                    <image_roi>
                        <x>187</x>
